{
    "name": "vscode-open-in-stash",
    "displayName": "vscode-open-in-stash",
    "description": "Open vscode in Stash Atlassian",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.29.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.openInStash"
    ],
    "main": "./out/extension",
    "properties": {
        "openInStash.stashDomain": {
          "type": [
            "string"
          ],
          "default": "",
          "description": "Configure a custom Stash domain. Useful for Bitbucket Atlassian enterprise"
        }
    },
    "contributes": {
        "commands": [
            {
                "command": "extension.openInStash",
                "title": "Open in Stash"
            }, 
            {
                "command": "extension.openInStashPR",
                "title": "Open in Stash PR"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "yarn run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "yarn run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^8.10.25",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.21"
    },
    "dependencies": {
        "@types/find-parent-dir": "^0.3.0",
        "@types/git-branch": "^2.0.0",
        "@types/git-url-parse": "^9.0.0",
        "@types/open": "^0.0.29",
        "find-parent-dir": "^0.3.0",
        "git-branch": "^2.0.1",
        "git-repo-info": "^2.0.0",
        "git-url-parse": "^11.1.1",
        "gitconfiglocal": "^2.0.2",
        "open": "^0.0.5",
        "parse-git-config": "^2.0.3",
        "pify": "^4.0.1"
    }
}
